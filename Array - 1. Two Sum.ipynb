    "## Hashmap solution"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def twoSum(self, nums, target):\n",
    "        seen = {}\n",
    "        for i, num in enumerate(nums):\n",
    "            if target - num in seen:\n",
    "                return [seen[target - num], i]\n",
    "            seen[num] = i\n",
    "        return -1"
   ]
