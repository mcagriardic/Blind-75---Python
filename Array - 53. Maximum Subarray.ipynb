   "source": [
    "class Solution:\n",
    "    def maxSubArray(self, nums):\n",
    "        ans = nums[0]\n",
    "        dp = [nums[0]] + [[]] * (len(nums) - 1)\n",
    "        for i in range(1, len(nums)):\n",
    "            dp[i] = max(dp[i-1] + nums[i], nums[i])\n",
    "            ans = max(ans, dp[i])\n",
    "        return ans"
   ]
  }
